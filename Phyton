def calculate_grade(score):
    if score >= 90:
        return "A"
    elif score >= 80:
        return "B"
    elif score >= 70:
        return "C"
    elif score >= 60:
        return "D"
    else:
        return "F"

def main():
    student_name = input("Enter student name: ")
    score = float(input("Enter score: "))
    grade = calculate_grade(score)
    print(f"Student Name: {student_name}")
    print(f"Score: {score}")
    print(f"Grade: {grade}")

if __name__ == "__main__":
    main()
